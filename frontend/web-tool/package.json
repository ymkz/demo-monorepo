{
  "name": "web-tool",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev": "wireit",
    "build": "wireit",
    "typecheck": "wireit",
    "test": "wireit",
    "coverage": "wireit"
  },
  "wireit": {
    "dev": {
      "command": "next dev --turbopack -p 4000"
    },
    "build": {
      "command": "next build"
    },
    "typecheck": {
      "command": "tsc --noEmit"
    },
    "test": {
      "command": "vitest --run"
    },
    "coverage": {
      "command": "vitest --run --coverage"
    }
  },
  "dependencies": {
    "httpxy": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "next": "catalog:",
    "nuqs": "catalog:"
  }
}
